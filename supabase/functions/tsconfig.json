{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext", "webworker"],
    "moduleResolution": "node",
    "module": "esnext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "types": []
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules"]
}